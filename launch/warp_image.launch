<launch>
    <node name="warp_image_node" pkg="drive_ros_image_recognition" type="warp_image_node" respawn="false" output="screen">
        <rosparam command="load" file="$(find drive_ros_image_recognition)/homography_matrix.yaml"/>
        <rosparam command="load" file="$(find drive_ros_image_recognition)/camera_matrix.yaml"/>
        <remap from="~/img_in" to="/camera1/image_raw"/>
    </node>
</launch>
